<connector xmlns="http://java.sun.com/xml/ns/j2ee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
           http://java.sun.com/xml/ns/j2ee/connector_1_5.xsd"
           version="1.5">

   <vendor-name>Red Hat Middleware LLC</vendor-name>
   <eis-type>Test RA</eis-type>
   <resourceadapter-version>0.1</resourceadapter-version>
   <resourceadapter>
      <resourceadapter-class>org.jboss.jca.adapters.sap.JBossSAPResourceAdapter</resourceadapter-class>
      <outbound-resourceadapter>
         <connection-definition>
            <managedconnectionfactory-class>org.jboss.jca.adapters.sap.JBossSAPManagedConnectionFactory</managedconnectionfactory-class>
            <connectionfactory-interface>javax.resource.cci.ConnectionFactory</connectionfactory-interface>
            <connectionfactory-impl-class>org.jboss.jca.adapters.sap.JBossSAPCciConnectionFactory</connectionfactory-impl-class>
            <connection-interface>javax.resource.cci.Connection</connection-interface>
            <connection-impl-class>org.jboss.jca.adapters.sap.JBossSAPCciConnection</connection-impl-class>
            <!-- 
            <config-property>
            	<config-property-name>destinationName</config-property-name>
            	<config-property-type>java.lang.String</config-property-type>
            	<config-property-value>default-destination</config-property-value>
            </config-property>
             -->
            <config-property>
            	<config-property-name>ashost</config-property-name>
            	<config-property-type>java.lang.String</config-property-type>
            	<config-property-value>appserver</config-property-value>
            </config-property>
            <config-property>
            	<config-property-name>sysnr</config-property-name>
            	<config-property-type>java.lang.String</config-property-type>
            	<config-property-value>01</config-property-value>
            </config-property>
            <config-property>
            	<config-property-name>client</config-property-name>
            	<config-property-type>java.lang.String</config-property-type>
            	<config-property-value>000</config-property-value>
            </config-property>
            <config-property>
            	<config-property-name>user</config-property-name>
            	<config-property-type>java.lang.String</config-property-type>
            	<config-property-value>JCOTESTER</config-property-value>
            </config-property>
            <config-property>
            	<config-property-name>passwd</config-property-name>
            	<config-property-type>java.lang.String</config-property-type>
            	<config-property-value>JCOTESTERPASSWORD</config-property-value>
            </config-property>
            <config-property>
            	<config-property-name>lang</config-property-name>
            	<config-property-type>java.lang.String</config-property-type>
            	<config-property-value>en</config-property-value>
            </config-property>
         </connection-definition>
         <transaction-support>NoTransaction</transaction-support>
         <reauthentication-support>false</reauthentication-support>
      </outbound-resourceadapter>
   </resourceadapter>
</connector>
