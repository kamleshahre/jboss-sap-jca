<ironjacamar xmlns="http://www.jboss.org/ironjacamar/schema"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://www.jboss.org/ironjacamar/schema 
             http://www.jboss.org/ironjacamar/schema/ironjacamar_1_0.xsd">

  <transaction-support>NoTransaction</transaction-support>
  
  <connection-definitions>
    <connection-definition class-name="org.jboss.jca.adapters.sap.JBossSAPManagedConnectionFactory" jndi-name="java:/eis/JBossSAPCciConnectionFactory" pool-name="JBossSAPCciConnectionFactory">
    	<config-property name="ashost">appserver</config-property>
    	<config-property name="sysnr">01</config-property>
    	<config-property name="client">000</config-property>
    	<config-property name="user">JCOTESTER</config-property>
    	<config-property name="passwd">JCOTESTERPASSWORD</config-property>
    	<config-property name="lang">en</config-property>
    </connection-definition>
    <connection-definition class-name="org.jboss.jca.adapters.sap.JBossSAPManagedConnectionFactory" jndi-name="java:/eis/TestConfigurationConnectionFactory" pool-name="JBossSAPCciConnectionFactory">
    	<config-property name="authType">CONFIGURED_USER</config-property>
    	<config-property name="userId">JCO_TESTER_USER_ID</config-property>
    	<config-property name="client">999</config-property>
    	<config-property name="user">JCO_TESTER_USER</config-property>
    	<config-property name="aliasUser">JCO_TESTER_ALIAS_USER</config-property>
    	<config-property name="passwd">JCO_TESTER_PASSWD</config-property>
    	<config-property name="lang">fr</config-property>
    	<config-property name="mysapsso2">JCO_TESTER_SAP_COOKIE</config-property>
    	<config-property name="x509cert">JCO_TESTER_X509_CERT</config-property>
    	<config-property name="saprouter">JCO_TESTER_SAP_ROUTER</config-property>
    	<config-property name="sysnr">99</config-property>
    	<config-property name="ashost">jcotesterashost.example.com</config-property>
    	<config-property name="mshost">jcotestermshost.example.com</config-property>
    	<config-property name="msserv">1234</config-property>
    	<config-property name="gwhost">jcotestergwhost.example.com</config-property>
    	<config-property name="gwserv">4321</config-property>
    	<config-property name="r3name">T11</config-property>
    	<config-property name="group">LOGONGROUP</config-property>
    	<config-property name="trace">1</config-property>
    	<config-property name="cpicTrace">9</config-property>
    	<config-property name="lcheck">1</config-property>
    	<config-property name="useSapgui">2</config-property>
    	<config-property name="codepage">ISO-8859-2</config-property>
    	<config-property name="getsso2">1</config-property>
    	<config-property name="denyInitialPassword">1</config-property>
    	<config-property name="peakLimit">99</config-property>
    	<config-property name="poolCapacity">199</config-property>
    	<config-property name="expirationTime">20000</config-property>
    	<config-property name="expirationPeriod">40000</config-property>
    	<config-property name="maxGetTime">1000</config-property>
    	<config-property name="sncMode">1</config-property>
    	<config-property name="sncPartnername">CN=R3, O=XYZ-INC, C=EN</config-property>
    	<config-property name="sncQop">9</config-property>
    	<config-property name="sncMyname">CN=R3, O=UVW-INC, C=EN</config-property>
    	<config-property name="sncLibrary">C:\SECUDE\LIB\SECUDE.DLL</config-property>
    	<config-property name="repositoryDest">TEST</config-property>
    	<config-property name="repositoryUser">JCO_REPOSITORY_TESTER</config-property>
    	<config-property name="repositoryPasswd">JCO_REPOSITORY_PASSWD</config-property>
    	<config-property name="repositorySnc">0</config-property>
    	<config-property name="repositoryRoundtripOptimization">1</config-property>
    </connection-definition>
  </connection-definitions>


</ironjacamar>
